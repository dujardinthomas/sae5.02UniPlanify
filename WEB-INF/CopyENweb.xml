<!-- FICHIER DE CONFIGURATION LOCAL NON IMPORTE SUR GIT -->
<web-app>

    <context-param>
        <param-name>driver</param-name>
        <param-value>org.postgresql.Driver</param-value>
    </context-param>

    <context-param>
        <param-name>url</param-name>
        <!--  URL DE VOTRE BASE SQL -->
        <param-value>jdbc:postgresql://localhost/uniplanify</param-value>
    </context-param>

    <context-param>
        <param-name>login</param-name>
        <!--  LOGIN DE VOTRE BASE SQL -->
        <param-value>thoma</param-value>
    </context-param>

    <context-param>
        <param-name>password</param-name>
        <!--  PASSWORD DE VOTRE BASE SQL -->
        <param-value>thoma</param-value>
    </context-param>


    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>


</web-app>